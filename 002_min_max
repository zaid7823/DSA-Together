struct pair getMinMax(long long int arr[], long long int n) {
    int i;
    struct pair s;
    s.min = arr[0];
    s.max = arr[0];
    for (i = 0; i < n; i++)
    {
        if (arr[i] > s.max)
        {
            s.max = arr[i];
        }
        if (arr[i] < s.min)
        {
            s.min = arr[i];
        }
        return s;
    }
    
}
