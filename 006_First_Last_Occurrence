// Finding the first and last occurrence of an int element in an array

#include<bits/stdc++.h>
using namespace std;

vector<int> find(int arr[], int n , int x )
{
    int first = -1, last = -1, i, j, count;
    for (i = 0; i < n; i++)
    {
        if (x == arr[i])
        {
            if (first == -1)
            {first = i;}
            else{continue;}
            count++;
        }
    }
    
    int c = count;
    count = 0;
    for (j = 0; j < n; j++)
    {
        if (x == arr[j])
        {
            count++;
            if (count == c);
            last = j;
        }
    }
    return {first, last};
}

// Driver Code

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,x;
        cin>>n>>x;
        int arr[n],i;
        for(i=0;i<n;i++)
        cin>>arr[i];
        vector<int> ans;
        ans=find(arr,n,x);
        cout<<ans[0]<<" "<<ans[1]<<endl;
    }
    return 0;
}
